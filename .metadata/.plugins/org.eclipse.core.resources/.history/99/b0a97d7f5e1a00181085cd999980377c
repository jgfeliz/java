package graficos;

import java.awt.Image;
import java.awt.Toolkit;

import javax.swing.JFrame;

public class CambioEstado {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		marco marcoPrincipal=new marco();
		marcoPrincipal.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		marcoPrincipal.setVisible(true);

	}

}

class marco extends JFrame{
	
	public marco(){
		
		setBounds(400,400,400,400);
		setTitle("Marco X...");
		Toolkit icono=Toolkit.getDefaultToolkit();
		Image imagenIcono=icono.getImage("imagenes/icono.png");
		setIconImage(imagenIcono);
		
	}
	
}